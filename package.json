{
  "name": "markdown-component-loader",
  "version": "0.0.4",
  "description": "Turn Markdown into dynamic, stateless React components",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib --source-maps inline --ignore \"**/*.spec.js,**/__fixtures__,**/__mocks__\"",
    "test": "jest",
    "build": "webpack",
    "start": "webpack-dev-server",
    "lint": "eslint .",
    "lint-and-fixup": "eslint --fix ."
  },
  "keywords": [
    "markdown",
    "markdown-it",
    "react",
    "jsx"
  ],
  "homepage": "https://github.com/ticky/markdown-component-loader",
  "bugs": {
    "url": "https://github.com/ticky/markdown-component-loader/issues"
  },
  "repository": "https://github.com/ticky/markdown-component-loader.git",
  "author": "Jessica Stokes <hello@jessicastokes.net>",
  "license": "MIT",
  "dependencies": {
    "doc-chomp": "1.1.0",
    "front-matter": "^2.1.0",
    "highlight.js": "^9.6.0",
    "loader-utils": "^0.2.16",
    "markdown-it": "^7.0.1",
    "markdown-it-regexp": "^0.4.0",
    "sha.js": "^2.4.5"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jest": "^16.0.1",
    "json-loader": "^0.5.4",
    "query-params": "^0.0.1",
    "react": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "peerDependencies": {
    "react": "^15.3.2"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ]
  }
}
