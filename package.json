{
  "name": "markdown-component-loader",
  "version": "0.8.0",
  "description": "Turn Markdown into dynamic, stateless React components",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib --source-maps inline --ignore \"**/*.spec.js,**/__fixtures__,**/__mocks__\"",
    "test": "jest",
    "build": "webpack",
    "start": "webpack-dev-server",
    "lint": "eslint .",
    "lint-and-fixup": "eslint --fix ."
  },
  "keywords": [
    "markdown",
    "markdown-component",
    "markdown-it",
    "component",
    "webpack",
    "react",
    "jsx"
  ],
  "homepage": "https://github.com/ticky/markdown-component-loader",
  "bugs": {
    "url": "https://github.com/ticky/markdown-component-loader/issues"
  },
  "repository": "https://github.com/ticky/markdown-component-loader.git",
  "author": "Jessica Stokes <hello@jessicastokes.net>",
  "license": "MIT",
  "dependencies": {
    "any-base": "^1.0.0",
    "doc-chomp": "1.1.0",
    "front-matter": "^2.1.0",
    "highlight.js": "^9.6.0",
    "html-void-elements": "^1.0.1",
    "jsesc": "^2.2.0",
    "loader-utils": "^0.2.16",
    "markdown-it": "^8.1.0",
    "sha.js": "^2.4.5",
    "tsxml": "^0.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.11.1",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.0.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "markdown-it-anchor": "^2.5.1",
    "markdown-it-table-of-contents": "^0.3.0",
    "prop-types": "^15.5.10",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "react-test-renderer": "^15.3.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.3.2"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ]
  }
}
